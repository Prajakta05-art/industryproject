pipeline{
	agent any
	stages
	{
		stage('Code Checkout')
		{
			steps
			{
				git url: 'https://github.com/Prajakta05-art/industryproject.git', branch: 'main'
			}
		}
		
		stage('Code Compile')
		{
			steps
			{
				sh 'mvn compile'
			}
		}

		stage('Unit Test')
		{
			steps
			{
				sh 'mvn test'
			}
		}

		stage('Code packaging')
		{
			steps
			{
				sh 'mvn package'
			}
		}
   }
}